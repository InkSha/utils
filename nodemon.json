{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/",
    "dist/",
    "coverage/"
  ],
  "watch": [
    "src/",
    "src/index.ts",
    "src/index.mjs"
  ],
  "execMap": {
    "ts": "node --loader ts-node/esm"
  },
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js,json,ts"
}
